	<vte:include file="{VIVVO_FS_ADMIN_DIR}templates/gui/html_tags_input.xml" items="{existing_tags}" label="{LNG_ADMIN_TAGS}" name="edit_article_tags" noscript="1" default_topic_name="{default_topic.get_name}" />
